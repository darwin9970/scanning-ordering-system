{
  "name": "qr-order-mini",
  "version": "1.0.0",
  "description": "桌台扫码点单小程序",
  "main": "main.js",
  "scripts": {
    "dev:mp-weixin": "uni -p mp-weixin",
    "build:mp-weixin": "uni build -p mp-weixin",
    "dev:h5": "uni -p h5",
    "build:h5": "uni build -p h5",
    "lint": "eslint --ext .js,.vue . --fix",
    "lint:check": "eslint --ext .js,.vue .",
    "format": "prettier --write \"**/*.{js,vue,json,css,scss}\" --ignore-path .gitignore",
    "format:check": "prettier --check \"**/*.{js,vue,json,css,scss}\" --ignore-path .gitignore"
  },
  "dependencies": {
    "@dcloudio/uni-app": "^3.0.0-4010520240507001",
    "@dcloudio/uni-components": "^3.0.0-4010520240507001",
    "@dcloudio/uni-h5": "^3.0.0-4010520240507001",
    "@dcloudio/uni-mp-weixin": "^3.0.0-4010520240507001",
    "@vitejs/plugin-vue": "^6.0.2",
    "pinia": "^2.1.7",
    "vue": "^3.4.21"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,scss}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@dcloudio/types": "^3.4.8",
    "@dcloudio/uni-automator": "^3.0.0-4010520240507001",
    "@dcloudio/uni-cli-shared": "^3.0.0-4010520240507001",
    "@dcloudio/uni-stacktracey": "^3.0.0-4010520240507001",
    "@dcloudio/vite-plugin-uni": "^3.0.0-4010520240507001",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-vue": "^9.26.0",
    "lint-staged": "^15.5.2",
    "prettier": "^3.8.1",
    "sass": "^1.77.0",
    "vite": "^5.2.11"
  }
}
